	<queryString>
		<![CDATA[SELECT
     fatura.`id` AS fatura_id,
     fatura.`Fatura` AS fatura_Fatura,
     fatura.`total` AS fatura_total,
     fatura.`troco` AS fatura_troco,
     fatura.`valor_pago` AS fatura_valor_pago,
     fatura.`cliente` AS fatura_cliente,
     fatura.`client_id` AS fatura_client_id,
     fatura.`data_insert` AS fatura_data_insert,
     fatura.`tipoPagamento` AS fatura_tipoPagamento,
     fatura.`funcionario_id` AS fatura_funcionario_id,
     item.`id` AS item_id,
     item.`quantity` AS item_quantity,
     item.`produto` AS item_produto,
     item.`total` AS item_total,
     item.`fatura_id` AS item_fatura_id,
     item.`product_id` AS item_product_id
FROM
     `fatura` fatura INNER JOIN `item` item ON fatura.`id` = item.`fatura_id`
	WHERE fatura.`id`=$P{id}]]>
	</queryString>
